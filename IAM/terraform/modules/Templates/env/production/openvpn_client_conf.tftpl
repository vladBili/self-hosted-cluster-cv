client
dev tun
proto udp
remote ${ec2_instances["bastion"]["bastion-0"].public_ip} 1194
resolv-retry infinite
nobind
persist-key
persist-tun
ca ca.crt
cert client1.domain.tld.crt
key client1.domain.tld.key
remote-cert-tls server
cipher AES-256-CBC
auth SHA256
verb 3

<ca>
{{ ca_cert }}
</ca>

<cert>
{{ client_cert }}
</cert>

<key>
{{ client_key }}
</key>